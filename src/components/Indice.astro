    <script>
        let activa: any;
        const planometro = document.getElementById('planometro');
        const linea1 = document.getElementById('linea1');
        const enlace1 = document.getElementById('enlace-1');

        const linea2 = document.getElementById('linea2');
        const enlace2 = document.getElementById('enlace-2');

        const linea3 = document.getElementById('linea3');
        const enlace3 = document.getElementById('enlace-3');

        const linea4 = document.getElementById('linea4');
        const enlace4 = document.getElementById('enlace-4');

        const linea5 = document.getElementById('linea5');
        const enlace5 = document.getElementById('enlace-5');

        const linea6 = document.getElementById('linea6');
        const enlace6 = document.getElementById('enlace-6');

        const linea7 = document.getElementById('linea7');
        const enlace7 = document.getElementById('enlace-7');

        const linea8 = document.getElementById('linea8');
        const enlace8 = document.getElementById('enlace-8');

        const linea9 = document.getElementById('linea9');
        const enlace9 = document.getElementById('enlace-9');
        
        const linea10 = document.getElementById('linea10');
        const enlace10 = document.getElementById('enlace-10');

        enlace1?.addEventListener('mouseenter', () => {
            lineaOn(linea1);
        })
        
        enlace2?.addEventListener('mouseenter', () => {
            lineaOn(linea2);
        })
        
        enlace3?.addEventListener('mouseenter', () => {
            lineaOn(linea3);
        })
        
        enlace4?.addEventListener('mouseenter', () => {
            lineaOn(linea4);
        })
        
        enlace5?.addEventListener('mouseenter', () => {
            lineaOn(linea5);
        })
        
        enlace6?.addEventListener('mouseenter', () => {
            lineaOn(linea6);
        })
        
        enlace7?.addEventListener('mouseenter', () => {
            lineaOn(linea7);
        })
        
        enlace8?.addEventListener('mouseenter', () => {
            lineaOn(linea8);
        })
        
        enlace9?.addEventListener('mouseenter', () => {
            lineaOn(linea9);
        })
        
        enlace10?.addEventListener('mouseenter', () => {
            lineaOn(linea10);
        })
        
        function lineaOn(elem: any) {
            if (activa) {
                activa.setAttribute("class", "stroke-white");
            }

            planometro?.removeChild(elem);
            planometro?.appendChild(elem);

            elem.setAttribute("class", "stroke-red-700");
            activa = elem;
        }
    </script>
